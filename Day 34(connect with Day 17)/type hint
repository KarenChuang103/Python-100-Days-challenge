age: int
name: str
height: float
is_human: bool

# -------------------------

def greeting(name: str) --> str:
	return "Hello" + name

#name:str == declare type is str
#--> str == return type is str

def police_check(age: int) -> bool:
    if age > 18:
        can_drive = True
    else:
        can_drive = False
