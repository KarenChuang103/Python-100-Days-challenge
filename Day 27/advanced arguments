# KEYWORD ARGUMENTS

def my_function(a,b,c):
	#Do this with a
	#Then do this with b
	#Finally do this with c

my_function(c=3, a=1, b=2)

#HOW TO USE DEFAULT VALUES?

def my_function(a=1, b=2, c=3):
	#Do this with a
	#Then do this with b
	#Finally do this with c

my_function()
# means a=1, b=2, c=3 (default value) 

# IF I WANT TO CHANGE A DEFAULT VALUE
my_function(b=5)

